{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "suprematism-smart-text"
  },
  "apps": [
    {
      "stylePreprocessorOptions": {
        "includePaths": [
          "../node_modules/suprematism-style-guide/dist",
          "../node_modules/bootstrap-sass/assets/stylesheets/"
        ]
      },
      "root": "example",
      "outDir": "dist",
      "assets": [],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "supre",
      "styles": [
        "../node_modules/suprematism-style-guide/dist/suprematism.css"
      ],
      "scripts": [
        "../node_modules/shave/dist/shave.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "example/tsconfig.app.json"
    },
    {
      "project": "example/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}
