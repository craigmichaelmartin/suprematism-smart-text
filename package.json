{
  "name": "suprematism-smart-text",
  "version": "0.0.1",
  "license": "MIT",
  "angular-cli": {},
  "main": "out-tsc/index.js",
  "typings": "out-tsc/index.d.ts",
  "scripts": {
    "build": "npm run build:tsc && npm run build:css && npm run build:copyAssets",
    "build:copyAssets": "cp -R src/*.css ./out-tsc && cp -R src/*html ./out-tsc",
    "build:tsc": "tsc -p src",
    "build:css": "node-sass src -o src",
    "deploy": "npm run build && ng github-pages:deploy",
    "clean": "rm -rf out-tsc && find src -name '*.css' -delete",
    "e2e": "protractor",
    "example": "npm run watch & ng serve",
    "lint": "npm run lint:src && npm run lint:example",
    "lint:src": "tslint \"src/**/*.ts\"",
    "lint:example": "tslint \"example/**/*.ts\"",
    "prebuild": "npm run clean",
    "pree2e": "webdriver-manager update",
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "test": "ng test",
    "watch": "npm run watch:css",
    "watch:css": "node-sass -w src -o src"
  },
  "private": true,
  "peerDependencies": {
    "@angular/core": "^2.0.0",
    "ionicons": "^3.0.0"
  },
  "devDependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@types/jasmine": "^2.2.30",
    "angular-cli": "1.0.0-beta.16",
    "codelyzer": "~0.0.26",
    "core-js": "^2.4.1",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "4.0.9",
    "rxjs": "5.0.0-beta.12",
    "shave": "^0.2.0",
    "suprematism-popover": "github:cinbcuniversal/suprematism-popover",
    "ts-helpers": "^1.1.1",
    "ts-node": "1.2.1",
    "tslint": "3.13.0",
    "typescript": "2.0.2",
    "zone.js": "^0.6.23"
  },
  "dependencies": {
    "suprematism-scss": "github:cinbcuniversal/suprematism-scss#feature/colors"
  }
}
